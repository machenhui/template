.alipay{
  margin:0px;
}
.alipay .color_aaa{
	color:#aaa;
}
.alipay .align_left{
	text-align:left;
}
.alipay header {
	@mixin gradient(0 0, 0 100%, #ffffff,#f3f3f3);
	padding:0.5em;
	border-bottom:5px solid rgba(234,84,4,0.75);
	@mixin flexBox();
}
.alipay header *{
	vertical-align:middle;
	display:block;
	@mixin boxFlex(1);
}
.alipay header i.img.zfb{
	width:5.375em;
	height:1.685em;
	background:center center no-repeat;
	background-size:100% 100%;
	background-image:url(data:image/png;base64,);
	display:inline-block;
}

.alipay header .desc{
	/**float:right;**/
	color:#888;
	text-align:right;
}

.alipay .tip{
	/*width:19em;
	margin:auto;*/
	margin-top:1em;
	text-align:center;
	line-height:1.5;
}
.alipay .tip>*{
	vertical-align:middle;
	display:inline-block;
}
.alipay .tip i.icon{
	width:3em;
	height:3em;
	background:center center no-repeat;
	background-size:100% 100%;
	display:inline-block;
}
.alipay .tip i.icon.waring{
	
	background-image:url(data:image/png;base64,);
	
}

.kuaichong_icon{
	width:90px;
	height:40px;
	display:block;
	float:right;
    background-image:url(data:image/png;base64,);    
}

.alipay .tip i.icon.success{
		background-image:url(data:image/png;base64,);	
}
.alipay .tip i.icon.success+p{
	font-size:1.5em;
	font-weight:bold;
}
.alipay .tip .desc{
	display:inline-block;
	color:#000;
	width:auto;
	
}
.alipay .tip .desc.quit{
	max-width:70%;
}
.alipay .tip .desc span:first-of-type{
	display:inline-block;
}
.alipay .tip .desc span{
	display:block;
}

/**********核心信息内容*************/
.alipay .tip_message{
	padding:0.5em;
	border-bottom:10px solid transparent;
	 
	 @mixin gradient(0 0, 0 100%,#fffcf0,#ffffff);
	 @mixin borderImage(url(data:image/png;base64,) 20 0 repeat);
	
}

.alipay .tip_message a{
	float:right;
}
.alipay .content{
	padding:0.5em;
}
/*********table 区域*********/
.alipay .content table{
	line-height:1.5;
}
.alipay .content table tr td:last-of-type{
	/*padding-left:0.7em;*/
}
/**********buttons***********/
.alipay .buttons {
   font-size:1.5em;
}
.alipay .buttons .btn{
   border-radius:2px;
   display:block;
   padding:0.5em;
   margin:1em 0px;
   text-align:center;
   color:#fff;
   text-shadow:0px 1px 1px  #aaa; 
   @mixin gradient(0 0, 0 100%,#ff761b,#f96f00);
   /**十分悲剧的多个值问题***/
   @def TEMP_BOX_SHADOE_VALUE_1 0px 4px 1px 0px #a8460b ,0px 2px 0px 0px rgba(255,255,255,0.3) inset;
   @mixin boxShadow(TEMP_BOX_SHADOE_VALUE_1);
   border:1px solid #d0670a;
}
.alipay .buttons .btn.active,
.alipay .buttons .btn:active{
	@mixin gradient(0 0, 0 100%,#f96f00,#ff761b);
	
}
.alipay .buttons .btn.resend{
	border:1px solid #a0bcd1;
	@mixin gradient(0 0, 0 100%,#f9f9f9,#c9ddf0);
	 /**十分悲剧的多个值问题***/
   @def TEMP_BOX_SHADOE_VALUE_2 0px 2px 0px 0px #fff inset;
   @mixin boxShadow(TEMP_BOX_SHADOE_VALUE_2);
   color:#004bb5;
}
.alipay .buttons .btn.resend.active,
.alipay .buttons .btn.resend:active{
	
	@mixin gradient(0 0, 0 100%,#c9ddf0,#f9f9f9);
	
}

.alipay .buttons .btn[disabled]:active,
.alipay .buttons .btn[disabled]{
   background-color:#e6e5e5;
   border:1px solid #ddd;
   
   background-image:none; 
   -webkit-box-shadow:none;
}


/***********Form****************/
.alipay .date{
    @mixin flexBox();
}
.alipay .form input[type="text"],
.alipay .form input[type="number"],
.alipay .form input[type="tel"],
.alipay .date  select{
  border-radius:2px;
  padding:0.5em 1em;
  border:1px solid #dacec3;
  background-color:#fff;
  /**十分悲剧的多个值问题***/
   @def TEMP_BOX_SHADOE_VALUE_3 0px 2px 0px 0px rgba(238,218,205,0.4)  inset;
   @mixin boxShadow(TEMP_BOX_SHADOE_VALUE_3);
}
.alipay .date  .form-select::after{
   right:1em;
}
.alipay .date .form-select{	
	@mixin boxFlex(1);
	display:block;
}
.alipay .date .form-select:first-of-type{
	padding-right:0.5em;
}
.alipay .date .form-select:last-of-type{
	padding-left:0.5em;
}


.alipay .form input[type="text"],
.alipay .form input[type="submit"],
.alipay .form input[type="number"],
.alipay .form input[type="tel"]{
   display:block;
   width:100%;
}
.alipay .form input[type="submit"]{
	font-size:100%;
}
.alipay .form input[type="checkbox"]{
   padding:0px;
   margin-right:0.6em;
}
.alipay .form input,
.alipay .form .date{
    margin:0.5em 0px;
}
.alipay .license>*{
	vertical-align:middle;
	display:inline-block;
}
/**alipay 支付协议**/
.alipay.license_page .message p{
	text-indent:2em;
	padding:0.5em 1em;
	line-height:1.5;
}