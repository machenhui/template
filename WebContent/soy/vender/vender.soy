{namespace examples.simple}
/**
 * Says hello to the world.
 * @param srcMap
 */
{template .helloWorld}
  <%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
  <%@page import="java.util.LinkedHashMap" %>
  <%
	LinkedHashMap<String,String> src_map = new LinkedHashMap<String,String>();
	{foreach $key in keys($srcMap)}
		{if $srcMap[$key]}
			src_map.put("{$key}", "{$srcMap[$key]}");
		{/if}
		{ifempty}
	{/foreach}
	 
  %>
{/template}