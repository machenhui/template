{namespace quadManager.talk}
/**
 * @param item 显示的元素
 */
{template .listItem}
<tr id="{$item['id']}">
      <td>{$item['id']}</td>
      <td>{$item['message']}</td>
      <td><a href="javascript:quadJS.Chat.showUpdateForm({$item['id']});">更新</a>&nbsp;<a href="javascript:void(0);" onclick="javascript:quadJS.Chat.deleteChat({$item['id']},this);">删除</a></td>
    </tr>
{/template}