{namespace quadManager.watchUser}
/**
 * @param item 显示的元素
 */
{template .userListItem}
<tr id="{$item}">
      <td>{$item}</td>
      <td><a href="javascript:void(0);" onclick="javascript:quadJS.WatchUser.watch({$item},this);">监控</a>&nbsp;<a href="javascript:void(0);" onclick="javascript:quadJS.WatchUser.deleteUser({$item},this);">删除</a></td>
</tr>
{/template}